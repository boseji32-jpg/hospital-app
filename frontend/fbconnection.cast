[90.182662, "o", "# 3. Auth Context\r\n\rcat > src/context/AuthContext.tsx << 'EOF'\r\n\rimport { createContext, useContext, useState, ReactNode } from \"react\"\r\n\rimport api from \"../lib/api\"\r\n\rimport { LoginResponse, User } from \"../types\"\r\n\r\n\rinterface AuthContextType {\r\n\r  user: User | null\r\n\r  login: (email: string, password: string) => Promise<void>\r\n\r  logout: () => void\r\n\r  isLoading: boolean\r\n\r}\r\n\r\n\rconst AuthContext = createContext<AuthContextType | null>(null)\r\n\r\n\rexport function AuthProvider({ children }: { children: ReactNode }) {\r\n\r  const [user, setUser] = useState<User | null>(null)\r\n\r  const [isLoading, setIsLoading] = useState(false)\r  const login = async (email: string, password: string) => {\r    setIsLoading(true)\r    try {\r      const formData = new FormData()\r      formData.append(\"username\", email)\r      formData.append(\"password\", password)\r      const response = await api.post<LoginResponse>(\"/auth/login\", formData, {\r        headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\r      })\r      localStorage.setItem(\"token\", response.data.access_token)\r      setUser(response.data.user)\r    } finally {\r      setIsLoading(false)\r    }\r  }\r\u001b[7m  const logout = () => {\u001b[27m\r\u001b[7m    localStorage.removeItem(\"token\")\u001b[27m\r\u001b[7m    setUser(null)\u001b[27m\r\u001b[7m  }\u001b[27m\r\u001b[7m  return (\u001b[27m\r\u001b[7m    <AuthContext.Provider value={{ user, login, logout, isLoading }}>\u001b[27m\r\u001b[7m      {children}\u001b[27m\r\u001b[7m    </AuthContext.Provider>\u001b[27m\r\u001b[7m  )\u001b[27m\r\u001b[7m}\u001b[27m\r\u001b[7mexport const useAuth = () => {\u001b[27m\r\u001b[7m  const context = useContext(AuthContext)\u001b[27m\r\u001b[7m  if (!context) throw new Error(\"useAuth must be used within AuthProvider\")\u001b[27m\r\u001b[7m  return context\u001b[27m\r\u001b[7m}\u001b[27m\r\u001b[7mEOF\u001b[27m"]
[90.187195, "o", "\r  const logout = () => {\r    localStorage.removeItem(\"token\")\r    setUser(null)\r  }\r  return (\r    <AuthContext.Provider value={{ user, login, logout, isLoading }}>\r      {children}\r    </AuthContext.Provider>\r  )\r}\rexport const useAuth = () => {\r  const context = useContext(AuthContext)\r  if (!context) throw new Error(\"useAuth must be used within AuthProvider\")\r  return context\r}\rEOF\r\n\u001b[?2004l\r"]
[90.192502, "o", "\u001b[?2004h\u001b]0;prav@Praveen: ~/hospital-app/frontend\u0007\u001b[01;32mprav@Praveen\u001b[00m:\u001b[01;34m~/hospital-app/frontend\u001b[00m$ "]
[99.188728, "o", "\u001b[7m# 4. Protected Route\u001b[27m\r\n\r\u001b[7mcat > src/components/ProtectedRoute.tsx << 'EOF'\u001b[27m\r\n\r\u001b[7mimport { Navigate } from \"react-router-dom\"\u001b[27m\r\n\r\u001b[7mimport { useAuth } from \"../context/AuthContext\"\u001b[27m\r\n\r\n\r\u001b[7mexport default function ProtectedRoute({ children }: { children: ReactNode }) {\u001b[27m\r\n\r\u001b[7m  const { user } = useAuth()\u001b[27m\r\n\r\u001b[7m  return user ? <>{children}</> : <Navigate to=\"/login\" />\u001b[27m\r\n\r\u001b[7m}\u001b[27m\r\n\r\u001b[7mEOF\u001b[27m"]
[99.718051, "o", "\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\r\u001b]0;prav@Praveen: ~/hospital-app/frontend\u0007\u001b[01;32mprav@Praveen\u001b[00m:\u001b[01;34m~/hospital-app/frontend\u001b[00m$ # 4. Protected Route\r\n\rcat > src/components/ProtectedRoute.tsx << 'EOF'\r\n\rimport { Navigate } from \"react-router-dom\"\r\n\rimport { useAuth } from \"../context/AuthContext\"\r\n\r\n\rexport default function ProtectedRoute({ children }: { children: ReactNode }) {\r\n\r  const { user } = useAuth()\r\n\r  return user ? <>{children}</> : <Navigate to=\"/login\" />\r\n\r}\r\n\rEOF\r\n\u001b[?2004l\r"]
