[145.41437, "o", "# 8. Update App.tsx with routes\r\n\rcat > src/App.tsx << 'EOF'\r\n\rimport { Routes, Route } from \"react-router-dom\"\r\n\rimport Login from \"./pages/Login\"\r\n\rimport Dashboard from \"./pages/Dashboard\"\r\n\r\u001b[7mimport Patients from \"./pages/Patients\"\u001b[27m\r\n\r\u001b[7mimport ProtectedRoute from \"./components/ProtectedRoute\"\u001b[27m\r\n\r\u001b[7mimport Layout from \"./components/Layout\"\u001b[27m\r\n\r\n\r\u001b[7mfunction App() {\u001b[27m\r\n\r\u001b[7m  return (\u001b[27m\r\n\r\u001b[7m    <Routes>\u001b[27m\r\n\r\u001b[7m      <Route path=\"/login\" element={<Login />} />\u001b[27m\r\n\r\u001b[7m      <Route element={<ProtectedRoute><Layout /></ProtectedRoute>}>\u001b[27m\r\n\r\u001b[7m        <Route path=\"/\" element={<Dashboard />} />\u001b[27m\r\n\r\u001b[7m        <Route path=\"/patients\" element={<Patients />} />\u001b[27m\r\n\r\u001b[7m      </Route>\u001b[27m\r\n\r\u001b[7m    </Routes>\u001b[27m\r\n\r\u001b[7m  )\u001b[27m\r\u001b[7m}\u001b[27m\r\u001b[7mexport default App\u001b[27m\r\u001b[7mEOF\u001b[27m"]
[146.113123, "o", "\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\rimport Patients from \"./pages/Patients\"\r\n\rimport ProtectedRoute from \"./components/ProtectedRoute\"\r\n\rimport Layout from \"./components/Layout\"\r\n\r\n\rfunction App() {\r\n\r  return (\r\n\r    <Routes>\r\n\r      <Route path=\"/login\" element={<Login />} />\r\n\r      <Route element={<ProtectedRoute><Layout /></ProtectedRoute>}>\r\n\r        <Route path=\"/\" element={<Dashboard />} />\r\n\r        <Route path=\"/patients\" element={<Patients />} />\r\n\r      </Route>\r\n\r    </Routes>\r\n\r  )\r}\rexport default App\rEOF\r\n"]
