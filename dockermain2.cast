[214.69201, "o", "\u001b[7mcd ~/hospital-app\u001b[27m\r\n\r\n\r\u001b[7m# Build a brand-new, guaranteed-working frontend (no old images, no cache)\u001b[27m\r\n\r\u001b[7mdocker build -t hospital-frontend-working - <<'EOF'\u001b[27m\r\n\r\u001b[7mFROM node:20-alpine AS build\u001b[27m\r\n\r\u001b[7mWORKDIR /app\u001b[27m\r\n\r\u001b[7mCOPY frontend/package*.json ./\u001b[27m\r\n\r\u001b[7mRUN npm ci\u001b[27m\r\n\r\u001b[7mCOPY frontend .\u001b[27m\r\n\r\u001b[7mENV VITE_API_URL=http://127.0.0.1:8000/api\u001b[27m\r\n\r\u001b[7mRUN npm run build\u001b[27m\r\n\r\n\r\u001b[7mFROM nginx:alpine\u001b[27m\r\n\r\u001b[7mCOPY --from=build /app/dist /usr/share/nginx/html\u001b[27m\r\n\r\u001b[7mRUN echo 'server { listen 80; location / { root /usr/share/nginx/html; try_files $uri $uri/ /index.html; } }' > /etc/nginx/conf.d/default.conf\u001b[27m\r\n\r\u001b[7mEOF\u001b[27m\r\n\r\n\r\u001b[7m# Kill old containers and start fresh with host network\u001b[27m\r\n\r\u001b[7mdocker rm -f api web 2>/dev/null || true\u001b[27m\r\n\r\u001b[7mdocker run -d --name api --network host hospital-backend:latest\u001b[27m\r\n\r\u001b[7mdocker run -d --name web --network host hospital-frontend-working\u001b[27m"]
[215.293986, "o", "\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\r\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[C\u001b[Ccd ~/hospital-app\r\n\r\n\r# Build a brand-new, guaranteed-working frontend (no old images, no cache)\r\n\rdocker build -t hospital-frontend-working - <<'EOF'\r\n\rFROM node:20-alpine AS build\r\n\rWORKDIR /app\r\n\rCOPY frontend/package*.json ./\r\n\rRUN npm ci\r\n\rCOPY frontend .\r\n\rENV VITE_API_URL=http://127.0.0.1:8000/api\r\n\rRUN npm run build\r\n\r\n\rFROM nginx:alpine\r\n\rCOPY --from=build /app/dist /usr/share/nginx/html\r\n\rRUN echo 'server { listen 80; location / { root /usr/share/nginx/html; try_files $uri $uri/ /index.html; } }' > /etc/nginx/conf.d/default.conf\r\n\rEOF\r\n\r\n\r# Kill old containers and start fresh with host network\r\n\rdocker rm -f api web 2>/dev/null || true\r\n\rdocker run -d --name api --network host hospital-backend:latest\r\n\rdocker run -d --name web --network host hospital-frontend-working\r\n"]
