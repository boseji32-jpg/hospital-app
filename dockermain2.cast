[182.311664, "o", "# Kill old junk + build a brand-new, guaranteed-working frontend image\r\n\rdocker rm -f web api\r\n\rdocker build -t hospital-frontend-working ./frontend <<'EOF'\r\n\rFROM node:20-alpine AS build\r\n\r\u001b[7mWORKDIR /app\u001b[27m\r\n\r\u001b[7mCOPY frontend/package*.json ./\u001b[27m\r\n\r\u001b[7mRUN npm ci\u001b[27m\r\n\r\u001b[7mCOPY frontend/ .\u001b[27m\r\n\r\u001b[7mENV VITE_API_URL=http://127.0.0.1:8000/api\u001b[27m\r\n\r\u001b[7mRUN npm run build\u001b[27m\r\n\r\n\r\u001b[7mFROM nginx:alpine\u001b[27m\r\n\r\u001b[7mCOPY --from=build /app/dist /usr/share/nginx/html\u001b[27m\r\n\r\u001b[7mCOPY <<'NGINX' /etc/nginx/conf.d/default.conf\u001b[27m\r\n\r\u001b[7mserver {\u001b[27m\r\n\r\u001b[7m    listen 80;\u001b[27m\r\n\r\u001b[7m    location / { root /usr/share/nginx/html; try_files $uri $uri/ /index.html; }\u001b[27m\r\n\r\u001b[7m}\u001b[27m\r\n\r\u001b[7mNGINX\u001b[27m\r\n\r\u001b[7mEOF\u001b[27m\r\u001b[7m# Start both with host network\u001b[27m\r\u001b[7mdocker run -d --name api --network host hospital-backend:latest\u001b[27m\r\u001b[7mdocker run -d --name web --network host hospital-frontend-working\u001b[27m"]
[182.328602, "o", "\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\rWORKDIR /app\r\n\rCOPY frontend/package*.json ./\r\n\rRUN npm ci\r\n\rCOPY frontend/ .\r\n\rENV VITE_API_URL=http://127.0.0.1:8000/api\r\n\rRUN npm run build\r\n\r\n\rFROM nginx:alpine\r\n\rCOPY --from=build /app/dist /usr/share/nginx/html\r\n\rCOPY <<'NGINX' /etc/nginx/conf.d/default.conf\r\n\rserver {\r\n\r    listen 80;\r\n"]
