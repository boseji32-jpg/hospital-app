[1006.538166, "o", "\u001b[7m# 1. Stop everything old\u001b[27m\r\n\r\u001b[7mdocker rm -f api web 2>/dev/null || true\u001b[27m\r\n\r\n\r\u001b[7m# 2. Start backend on host network (port 8000)\u001b[27m\r\n\r\u001b[7mdocker run -d --name api --network host hospital-backend:latest\u001b[27m\r\n\r\n\r\u001b[7m# 3. Start a clean nginx that serves the working frontend on port 5173\u001b[27m\r\n\r\u001b[7mdocker run -d --name web --network host nginx:alpine sh -c \"\u001b[27m\r\n\r\u001b[7m  echo 'server { \u001b[27m\r\n\r\u001b[7m    listen 5173; \u001b[27m\r\n\r\u001b[7m    location / { \u001b[27m\r\n\r\u001b[7m      root /usr/share/nginx/html; \u001b[27m\r\n\r\u001b[7m      try_files \\$uri /index.html; \u001b[27m\r\n\r\u001b[7m    } \u001b[27m\r\n\r\u001b[7m  }' > /etc/nginx/conf.d/default.conf && \u001b[27m\r\n\r\u001b[7m  wget -qO- https://github.com/praveentech/hospital-frontend-dist/archive/main.tar.gz | \\\u001b[27m\r\n\r\u001b[7m    tar xz -C /usr/share/nginx/html --strip-components=1 hospital-frontend-dist-main/dist && \u001b[27m\r\n\r\u001b[7m  nginx -g 'daemon off;'\u001b[27m\r\n\r\u001b[7m\"\u001b[27m"]
[1007.508847, "o", "\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\u001b[A\r\u001b]0;prav@Praveen: ~\u0007\u001b[01;32mprav@Praveen\u001b[00m:\u001b[01;34m~\u001b[00m$ # 1. Stop everything old\r\n\rdocker rm -f api web 2>/dev/null || true\r\n\r\n\r# 2. Start backend on host network (port 8000)\r\n\rdocker run -d --name api --network host hospital-backend:latest\r\n\r\n\r# 3. Start a clean nginx that serves the working frontend on port 5173\r\n\rdocker run -d --name web --network host nginx:alpine sh -c \"\r\n\r  echo 'server { \r\n\r    listen 5173; \r\n\r    location / { \r\n\r      root /usr/share/nginx/html; \r\n\r      try_files \\$uri /index.html; \r\n\r    } \r\n\r  }' > /etc/nginx/conf.d/default.conf && \r\n\r  wget -qO- https://github.com/praveentech/hospital-frontend-dist/archive/main.tar.gz | \\\r\n\r    tar xz -C /usr/share/nginx/html --strip-components=1 hospital-frontend-dist-main/dist && \r\n\r  nginx -g 'daemon off;'\r\n\r\"\r\n\u001b[?2004l\r"]
